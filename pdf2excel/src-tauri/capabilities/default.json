{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save"
  ],
  "commands": {
    "allow": [
      "parse_pdf",
      "export_excel", 
      "check_python"
    ]
  }
}
